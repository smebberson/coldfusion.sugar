	<set name="com.scottmebberson.cfml.tag.cfloop.attributes">
		<completion string="index" />
		<completion string="from" />
		<completion string="to" />
		<completion string="step" />
	</set>
	
	<set name="com.scottmebberson.cfml.tag.cfhttp.attributes">
		<completion string="url" />
		<completion string="charset" />
		<completion string="clientCert" />
		<completion string="clientCertPassword" />
		<completion string="getAsBinary" />
		<completion string="method" />
		<completion string="password" />
		<completion string="port" />
		<completion string="proxyServer" />
		<completion string="proxyPort" />
		<completion string="proxyUser" />
		<completion string="proxyPassword" />
		<completion string="redirect" />
		<completion string="resolveURL" />
		<completion string="throwOnError" />
		<completion string="userAgent" />
		<completion string="username" />
	</set>
	
