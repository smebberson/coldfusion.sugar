<?xml version="1.0" encoding="UTF-8"?>
<!--

CodeSense Libraries simply define lists of completions, categorized by keys. They don't define any criteria for when they should be used; this is the domain of a CodeSense Provider.

-->
<codesense version="1.0">

	<behavior>
		<append-dynamic>
			<matched-suffix>(\s*=)|[^=]</matched-suffix>
			<transform-into>(?1::="$1"$0)</transform-into>
		</append-dynamic>
	</behavior>
	
	<set name="com.scottmebberson.cfml.tag.cfabort.attributes">
		<completion string="showError" />
	</set>
	
	<set name="com.scottmebberson.cfml.tag.cfajaximport.attributes">
		<completion string="cssSrc" />
		<completion string="scriptSrc" />
		<completion string="tags" />
	</set>
	
	<set name="com.scottmebberson.cfml.tag.cfajaxproxy.attributes">
		<completion string="cfc" />
		<completion string="jsclassname" />
		<!-- or -->
		<completion string="bind" />
		<completion string="onError" />
		<completion string="onSuccess" />
	</set>
	
	<set name="com.scottmebberson.cfml.tag.cfapplet.attributes">
		<completion string="appletSource" />
		<completion string="name" />
		<completion string="align" />
		<completion string="height" />
		<completion string="hSpace" />
		<completion string="notSupported" />
		<completion string="param_n" />
		<completion string="vSpace" />
		<completion string="width" />
	</set>
	
	<set name="com.scottmebberson.cfml.tag.cfapplication.attributes">
		<completion string="name" />
		<completion string="applicationTimeout" />
		<completion string="clientManagement" />
		<completion string="loginStorage" />
		<completion string="scriptProtect" />
		<completion string="secureJSON" />
		<completion string="secureJSONPrefix" />
		<completion string="sessionManagement" />
		<completion string="sessionTimeout" />
		<completion string="setClientCookies" />
		<completion string="setDomainCookies" />
	</set>
	
	<set name="com.scottmebberson.cfml.tag.cfargument.attributes">
		<completion string="name" />
		<completion string="default" />
		<completion string="displayname" />
		<completion string="hint" />
		<completion string="required" />
		<completion string="type" />
	</set>
	
	<set name="com.scottmebberson.cfml.tag.cfassociate.attributes">
		<completion string="baseTag" />
		<completion string="dataCollection" />
	</set>
	
	<set name="com.scottmebberson.cfml.tag.cfcache.attributes">
		<completion string="action" />
		<completion string="directory" />
		<completion string="expireURL" />
		<completion string="password" />
